<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<groups>
		<run>
			<include name="Smoke"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="WebAutomation.testComponents.Listeners"/>
	</listeners>
  <test thread-count="5" name="Login Validations">
    <classes>
      <class name="WebAutomation.tests.SuccessfulLoginTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
